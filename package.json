{
    "name": "keyswitch",
    "version": "0.6.0",
    "description": "",
    "main": "./src/main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "keywords": [],
    "author": "Mohammed Huseein Alsaggaf <moh.27@hotmail.com>",
    "license": "ISC",
    "build": {
        "appId": "com.moh27.keyboardSwitch",
        "productName": "Keyboard Switch",
        "directories": {
            "output": "../build/1"
        },
        "win": {
            "target": "nsis",
            "icon": "src/style/icon/icon.ico"
        },
        "nsis": {
            "installerIcon": "src/style/icon/icon.ico",
            "uninstallerIcon": "src/style/icon/uns.ico",
            "uninstallDisplayName": "Uninstall Keyboard Switch",
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    },
    "devDependencies": {
        "electron": "^11.2.3",
        "electron-builder": "^22.11.7",
        "electron-packager": "^15.3.0",
        "electron-debug": "^3.2.0",
        "electron-reload": "^1.5.0"
    },
    "dependencies": {
        "bulma": "^0.9.2",
        "bulma-divider": "^0.2.0",
        "bulma-switch": "^2.0.0",
        "electron-json-storage": "^4.5.0",
        "robotjs": "^0.6.0"
    }
}